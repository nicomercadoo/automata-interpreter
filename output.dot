digraph {
    rankdir = LR;
    inic [shape = point];
    edge [arrowsize = 0.8, penwidth = 0.8];
    node [shape = circle];

    // start state

    // transitions
    q0 -> q1 [label = "a"];
    q0 -> q2 [label = "b"];
    q1 -> q1 [label = "b"];
    q1 -> q3 [label = "a"];
    q2 -> q2 [label = "b"];
    q2 -> q3 [label = "a"];
    q3 -> q4 [label = "a, b"];
    q4 -> q4 [label = "a, b"];

    // final states
    q0 [shape = doublecircle];
    q1 [shape = doublecircle];
    q2 [shape = doublecircle];
    q3 [shape = doublecircle];

}
